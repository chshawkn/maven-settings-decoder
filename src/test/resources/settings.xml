<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                        http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <localRepository>${user.home}/.m2/repository</localRepository>
    <interactiveMode/>
    <usePluginRegistry/>
    <offline/>

    <proxies>
    </proxies>

    <mirrors>
        <mirror>
            <id>internal-nexus-mirror</id>
            <name>internal nexus mirror</name>
            <url>${internal-nexus.prefix}/nexus/content/groups/public/</url>
            <mirrorOf>central,spring-libs-release,spring-libs-snapshot</mirrorOf>
        </mirror>
    </mirrors>

    <servers>
        <server>
            <id>internal-nexus-releases</id>
            <username>deployment</username>
            <password>{RJzAlz+tw54HBhZ3IBzFCuNQY2Wyzqv2EonquIV7hdE=}</password>
        </server>
        <server>
            <id>internal-nexus-snapshots</id>
            <username>deployment</username>
            <password>{RJzAlz+tw54HBhZ3IBzFCuNQY2Wyzqv2EonquIV7hdE=}</password>
        </server>
        <server>
            <id>internal-mvnsite</id>
            <username>root</username>
            <privateKey>${user.home}/.ssh/internal-mvnsite</privateKey>
        </server>
    </servers>

    <activeProfiles>
        <activeProfile>internal-nexus</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>internal-nexus</id>
            <repositories>
                <repository>
                    <id>internal-nexus-public</id>
                    <name>internal nexus public</name>
                    <url>${internal-nexus.prefix}/nexus/content/groups/public</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </repository>
                <repository>
                    <id>internal-nexus-releases</id>
                    <name>internal nexus releases</name>
                    <url>${internal-nexus.prefix}/nexus/content/repositories/releases</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </repository>
                <repository>
                    <id>internal-nexus-snapshots</id>
                    <name>internal nexus snapshots</name>
                    <url>${internal-nexus.prefix}/nexus/content/repositories/snapshots</url>
                    <releases><enabled>false</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>internal-nexus-public</id>
                    <name>internal nexus public</name>
                    <url>${internal-nexus.prefix}/nexus/content/groups/public</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>internal-nexus-releases</id>
                    <name>internal nexus releases</name>
                    <url>${internal-nexus.prefix}/nexus/content/repositories/releases</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>internal-nexus-snapshots</id>
                    <name>internal nexus snapshots</name>
                    <url>${internal-nexus.prefix}/nexus/content/repositories/snapshots</url>
                    <releases><enabled>false</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>